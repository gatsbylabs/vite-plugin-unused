{
  "name": "vite-plugin-unused",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "author": "Enoch Chau <enoch@gatsby.events>",
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "fmt": "prettier --write .",
    "lint": "eslint src/ --fix",
    "typecheck": "tsc",
    "build": "tsc -p tsconfig.build.json",
    "test": "vitest run",
    "test:example": "cd ./example && vite build",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "prettier": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "pinst": ">=2",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vitest": "^0.12.9"
  },
  "peerDependencies": {
    "vite": "^2.9.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatsbylabs/vite-plugin-unused.git"
  },
  "keywords": [
    "vite",
    "plugin",
    "unused"
  ],
  "bugs": {
    "url": "https://github.com/gatsbylabs/vite-plugin-unused/issues"
  },
  "homepage": "https://github.com/gatsbylabs/vite-plugin-unused/#readme",
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "eslint --cache --fix",
    "*.{js,ts,tsx,jsx,json,md}": "prettier --write"
  }
}
